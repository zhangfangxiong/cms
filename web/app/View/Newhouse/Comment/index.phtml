    <script type="text/javascript">
        function saveFavorite() {
            if ("".length == 0) {
                alert("登录以后才能收藏，请先登录");
                return;
            }

            $.post("/user/favorite", {
                UnitId: "0fd910ed-6e86-4ea8-86b7-9c5545aacd5b",
                UnitName: "紫菁庭",
                UnitIntId: "410716",
                RegionName: "南汇",
                AnalystOpinion: "推荐购买",
                SalePrice: "18000",
                CricPrice: "19685",
                LevelCode: "A+",
                PicCode: "CRIC5e00184c9f643ef67777a0249d77f355",
                DistrictName: "临港新城",
                Category: "1",
                Url: location.href
            }, function (data) {
                if (data.dataCode == 1) {
                    $("#favorite_title").html("已收藏");
                    $("#collectStatus").children("a").addClass("add_acc").addClass("sf_acc");

                    //取消收藏功能绑定
                    $("#collectStatus").children("a").removeAttr("onclick");
                    $("#collectStatus").children("a").unbind("click");

                    setTimeout("changeClass()", 3000);
                }
                if (data.dataCode == 2) {
                    alert(data.errorMessage);
                    return;
                }
            });
        }

        function cancelUserFavorite() {
            $.post("/user/cancelfavorite", { unitId: "0fd910ed-6e86-4ea8-86b7-9c5545aacd5b" }, function (data) {
                if (data.dataCode == 1) {
                    $("#collectStatus").children("a").removeClass("sf_have");

                    setTimeout(function () {
                        $("#collectStatus").children("a").bind("click", saveFavorite);
                    }, 500);
                }
                else {
                    alert(data.errorMessage);
                }
            }, "json");
        }

        function changeClass() {
            $("#collectStatus").children("a").removeClass("sf_acc").removeClass("add_acc").addClass("sf_have");
        }

    </script>


