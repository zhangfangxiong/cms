    //等额本息
    function GetInterest(Interest, month) {
        //debugger;
        var a = 1 + Interest / 12;
        var number;
        for (var i = 0; i < month - 1; i++) {
            if (i == 0) {
                number = a * a;
            }
            if (i < month && i != 0) {
                number = (number * a);
            }
        }
        return number;
    }
    //精确小数点
    function round(v, e) {
        var t = 1;
        for (; e > 0; t *= 10, e--);
        for (; e < 0; t /= 10, e++);
        return Math.round(v * t) / t;

    }
    //等额本金利息
    function GetCapital(SumMoney, Interest, month) {
        //debugger;
        var a = Interest / 12;
        var Money;
        var Money1 = (SumMoney / month);
        var SumInterest;
        var monthInterest;
        for (var i = 0; i < month ; i++) {
            if (i == 0) {
                SumInterest = round((SumMoney * a), 2);
            }
            if (i < month && i != 0) {
                monthInterest = round(((SumMoney - Money1 * (i)) * a), 2);
                SumInterest += monthInterest;
            }
        }
        return SumInterest;
    }
